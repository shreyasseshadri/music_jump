 {
    "manifest_version": 2,
    "name": "Playlist Migrator",
	"version": "1.0",
	"description": "Migrates playlist",
	"permissions": ["tabs"],
	"icons": {
		"48": "icons/border-48.png"
	},
	"web_accessible_resources": [
		"content.js"
	],
    "content_scripts": [
      {
        "matches": [
			"*://music.amazon.in/*",
			"*://open.spotify.com/*"
        ],
		"js": [ 
			"contentScript.js"
		]
      }
    ]
  }