{
    "manifest_version": 2,
    "name": "Music Jump",
    "version": "0.1",
    "permissions": ["tabs"],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [ "content.js"]
      }
    ],
    "browser_action": {},
    "background": {
        "scripts": ["background.js"]
    }
  }